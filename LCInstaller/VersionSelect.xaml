<UserControl x:Class="LCInstaller.VersionSelect"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Height="20" Width="490">
    <Grid MouseDown="MouseDown">
        <TextBlock x:Name="LCVersion" MouseDown="MouseDown" HorizontalAlignment="Left" TextWrapping="Wrap" Text="LegenadryClient V2.0.0.0" VerticalAlignment="Top" Margin="10,2,0,0"/>
        <TextBlock x:Name="ExtraStuff" MouseDown="MouseDown" HorizontalAlignment="Right" TextWrapping="Wrap" Text="Prerelease" VerticalAlignment="Top" Margin="0,2,10,0"/>
        <TextBlock x:Name="DLTag" Visibility="Hidden" MouseDown="MouseDown" HorizontalAlignment="Left" TextWrapping="Wrap" Text="LegenadryClient V2.0.0.0" VerticalAlignment="Top" Margin="145,0,0,0"/>
        <TextBlock x:Name="VersionTag" Visibility="Hidden" MouseDown="MouseDown" HorizontalAlignment="Right" TextWrapping="Wrap" Text="Prerelease" VerticalAlignment="Top" Margin="0,2,9.28,0"/>

    </Grid>
</UserControl>
